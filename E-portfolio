  <script>
    // Replace simple youtu.be or watch?v links with proper embed URLs automatically
    function toEmbed(url){
      try{
        const u = new URL(url);
        if(u.hostname.includes('youtu.be')){ return 'https://www.youtube.com/embed/' + u.pathname.slice(1); }
        if(u.hostname.includes('youtube.com')){
          const id = u.searchParams.get('v');
          if(id) return 'https://www.youtube.com/embed/' + id;
        }
      }catch(e){}
      return url; // fallback (in case you already pasted embed URLs)
    }
    document.querySelectorAll('iframe[data-youtube]').forEach((el)=>{
      const raw = el.getAttribute('data-youtube');
      el.src = toEmbed(raw);
    });
    document.getElementById('year').textContent = new Date().getFullYear();
  </script>
